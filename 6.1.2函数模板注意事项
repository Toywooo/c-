#include<iostream>
using namespace std;
template<class T>//typename可以替换成class
void mySwap(T &a,T &b)
{
	T temp=a;
	a=b;
	b=temp;
}
//函数模板注意事项
//1.自动类型推导，必须推导出一致的数据类型T才可以使用
void test01()
{
	int a=10;
	int b=20;
	char c='c';
	//mySwap(a,b);//正确
	//mySwap(a,c);//错误，推导不出一致的T类型
	cout<<"a="<<a<<endl;
	cout<<"b="<<b<<endl;
}
//2.模板必须要确定出T的数据类型才可以使用
template<class T>//typename可以替换成class
void func()
{
	cout<<"func调用"<<endl;
}
void test02()
{
	//func();//编译器报错
	func<int>();//正确，指定T为int类型
}
int main()
{
	//test01();
	test02();
	system("pause");
	return 0;
}
